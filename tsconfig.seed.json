{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "outDir": "./dist",
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "prisma/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}